{
  "schema_version": 1,
  "quilt_loader": {
    "group": "io.github.ladysnake",
    "id": "lockii",
    "version": "${version}",
    "metadata": {
      "name": "Locki Test Mod",
      "description": "It's just a test... Or is it ?",
      "license": "LGPL-3.0-or-later",
      "icon": "assets/icon.png",
      "authors": [
        "Pyrofab"
      ],
      "contact": {
        "sources": "https://github.com/Ladysnake/Locki",
        "issues": "https://github.com/Ladysnake/Locki/issues",
        "discord": "ladysnake.glitch.me/discord"
      }
    },
    "entrypoints": {
      "main": [
        "io.github.ladysnake.lockii.Lockii"
      ],
      "fabric-gametest": [
        "io.github.ladysnake.lockii.tests.LockiTestSuite"
      ]
    },
    "depends": [
      "locki"
    ],
    "intermediate_mappings": "net.fabricmc:intermediary"
  },
  "minecraft": {
    "environment": "*"
  }
}
